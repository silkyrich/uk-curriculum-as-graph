  "templates": [
    {
      "name": "Show $subject $key_stage",
      "id": "tmpl:1",
      "createdAt": 1708387200000,
      "text": "Show $subject $key_stage",
      "cypher": "MATCH (p:Programme) WHERE p.subject_name=$subject AND p.key_stage=$key_stage MATCH path=(p)-[*1..4]->() RETURN path LIMIT 300",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Prerequisites for $concept",
      "id": "tmpl:2",
      "createdAt": 1708387200000,
      "text": "Prerequisites for $concept",
      "cypher": "MATCH path=(r:Concept)-[:PREREQUISITE_OF*]->(t:Concept) WHERE t.name CONTAINS $concept RETURN path LIMIT 100",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Year group profile $year",
      "id": "tmpl:3",
      "createdAt": 1708387200000,
      "text": "Year group profile $year",
      "cypher": "MATCH (y:Year {name:$year}) RETURN y",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "KS3 Science",
      "id": "tmpl:4",
      "createdAt": 1708387200000,
      "text": "KS3 Science",
      "cypher": "MATCH (ks:KeyStage {name:'KS3'})-[:HAS_YEAR]->(y)-[:HAS_PROGRAMME]->(p:Programme {subject_name:'Science'}) MATCH path=(p)-[*1..3]->() RETURN path LIMIT 200",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "$topic learning path",
      "id": "tmpl:5",
      "createdAt": 1708387200000,
      "text": "$topic learning path",
      "cypher": "MATCH (t:Topic)-[:TEACHES]->(c:Concept) WHERE t.topic_name CONTAINS $topic MATCH path=(c)-[:PREREQUISITE_OF*0..3]->() RETURN path LIMIT 100",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "All Y1 content",
      "id": "tmpl:6",
      "createdAt": 1708387200000,
      "text": "All Y1 content",
      "cypher": "MATCH (y:Year {name:'Y1'})-[:HAS_PROGRAMME]->(p)-[*1..3]->() RETURN p LIMIT 200",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Teaching clusters for $domain",
      "id": "tmpl:7",
      "createdAt": 1740268800000,
      "text": "Teaching clusters for $domain",
      "cypher": "MATCH (d:Domain) WHERE d.domain_id = $domain OR d.domain_name CONTAINS $domain MATCH path=(d)-[:HAS_CLUSTER]->(cc:ConceptCluster)-[:GROUPS]->(c:Concept) RETURN path LIMIT 150",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Cluster sequence for $domain",
      "id": "tmpl:8",
      "createdAt": 1740268800000,
      "text": "Cluster sequence for $domain",
      "cypher": "MATCH (d:Domain) WHERE d.domain_id = $domain MATCH path=(d)-[:HAS_CLUSTER]->(cc:ConceptCluster) OPTIONAL MATCH seq=(cc)-[:SEQUENCED_AFTER]->(cc2:ConceptCluster) RETURN path, seq LIMIT 100",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "EYFS to KS1 progression",
      "id": "tmpl:9",
      "createdAt": 1740268800000,
      "text": "EYFS to KS1 progression",
      "cypher": "MATCH path=(c1:Concept)-[:PREREQUISITE_OF]->(c2:Concept) WHERE c1.concept_id =~ '(CL|PSED|PD|LIT|EYMA|UW|EAD)-R-.*' RETURN path LIMIT 100",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Difficulty levels for $concept",
      "id": "tmpl:10",
      "createdAt": 1740268800000,
      "text": "Difficulty levels for $concept",
      "cypher": "MATCH (c:Concept) WHERE c.concept_id = $concept OR c.concept_name CONTAINS $concept MATCH path=(c)-[:HAS_DIFFICULTY_LEVEL]->(dl:DifficultyLevel) RETURN path ORDER BY dl.level_number LIMIT 20",
      "params": [],
      "hasCypherErrors": false
    }
