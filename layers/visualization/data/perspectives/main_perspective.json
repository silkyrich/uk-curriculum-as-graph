  "relationshipTypes": [],
  "templates": [
    {
      "name": "Prerequisites for $concept",
      "id": "tmpl:1",
      "createdAt": 1708387200000,
      "text": "Prerequisites for $concept",
      "cypher": "MATCH path=(r:Concept)-[:PREREQUISITE_OF*]->(t:Concept) WHERE t.name CONTAINS $concept RETURN path LIMIT 100",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Show $subject $key_stage",
      "id": "tmpl:2",
      "createdAt": 1708387200000,
      "text": "Show $subject $key_stage",
      "cypher": "MATCH (p:Programme) WHERE p.subject_name=$subject AND p.key_stage=$key_stage MATCH path=(p)-[*1..3]->() RETURN path LIMIT 200",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Show $layer layer",
      "id": "tmpl:3",
      "createdAt": 1708387200000,
      "text": "Show $layer layer",
      "cypher": "MATCH (n) WHERE n.display_category=$layer RETURN n LIMIT 300",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "What does $topic teach",
      "id": "tmpl:4",
      "createdAt": 1708387200000,
      "text": "What does $topic teach",
      "cypher": "MATCH (t:Topic)-[:TEACHES]->(c:Concept) WHERE t.topic_name CONTAINS $topic RETURN t, c LIMIT 100",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "NGSS vs UK science",
      "id": "tmpl:5",
      "createdAt": 1708387200000,
      "text": "NGSS vs UK science",
      "cypher": "MATCH (p:Practice) OPTIONAL MATCH (p)-[:ALIGNS_TO]->(c:Concept) RETURN p, c LIMIT 100",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Concepts assessed in KS2",
      "id": "tmpl:6",
      "createdAt": 1708387200000,
      "text": "Concepts assessed in KS2",
      "cypher": "MATCH (cdc:ContentDomainCode)-[:ASSESSES_CONCEPT]->(c:Concept) RETURN cdc, c LIMIT 200",
      "params": [],
      "hasCypherErrors": false
    }
  ],
