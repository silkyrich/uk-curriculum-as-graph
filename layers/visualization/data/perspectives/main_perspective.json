  "phrases": [
    {
      "phrase": "Prerequisites for $concept",
      "cypher": "MATCH path = (prereq:Concept)-[:PREREQUISITE_OF*]->(target:Concept) WHERE target.name CONTAINS $concept RETURN path LIMIT 100"
    },
    {
      "phrase": "Show $subject $key_stage",
      "cypher": "MATCH (p:Programme) WHERE p.subject_name = $subject AND p.key_stage = $key_stage MATCH path = (p)-[*1..3]->() RETURN path LIMIT 200"
    },
    {
      "phrase": "Show $layer layer",
      "cypher": "MATCH (n) WHERE n.display_category = $layer RETURN n LIMIT 300"
    },
    {
      "phrase": "What does $topic teach",
      "cypher": "MATCH (t:Topic)-[:TEACHES]->(c:Concept) WHERE t.topic_name CONTAINS $topic RETURN t, c LIMIT 100"
    },
    {
      "phrase": "NGSS practices",
      "cypher": "MATCH (d:Dimension)-[:HAS_PRACTICE]->(p:Practice) RETURN d, p"
    },
    {
      "phrase": "Compare NGSS to UK science",
      "cypher": "MATCH (p:Practice) OPTIONAL MATCH (p)-[:ALIGNS_TO]->(c:Concept) RETURN p, c LIMIT 100"
    },
    {
      "phrase": "KS2 test coverage",
      "cypher": "MATCH (tf:TestFramework)-[:HAS_PAPER]->(tp:TestPaper)-[:INCLUDES_CONTENT]->(cdc:ContentDomainCode) RETURN tf, tp, cdc LIMIT 200"
    },
    {
      "phrase": "Concepts assessed in KS2",
      "cypher": "MATCH (cdc:ContentDomainCode)-[:ASSESSES_CONCEPT]->(c:Concept) RETURN cdc, c LIMIT 200"
    }
  ]
