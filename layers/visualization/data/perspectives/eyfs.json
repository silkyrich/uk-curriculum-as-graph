  "templates": [
    {
      "name": "EYFS full structure",
      "id": "tmpl:1",
      "createdAt": 1740268800000,
      "text": "EYFS full structure",
      "cypher": "MATCH (ks:KeyStage {key_stage_id:'EYFS'})-[:HAS_YEAR]->(y)-[:HAS_PROGRAMME]->(p)-[:HAS_DOMAIN]->(d) RETURN ks, y, p, d LIMIT 200",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "EYFS area $area concepts",
      "id": "tmpl:2",
      "createdAt": 1740268800000,
      "text": "EYFS area $area concepts",
      "cypher": "MATCH (s:Subject) WHERE s.subject_id CONTAINS $area MATCH path=(s)<-[:FOR_SUBJECT]-(p:Programme {key_stage:'EYFS'})-[:HAS_DOMAIN]->(d)-[:HAS_CONCEPT]->(c:Concept) RETURN path LIMIT 150",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "EYFS to KS1 progression",
      "id": "tmpl:3",
      "createdAt": 1740268800000,
      "text": "EYFS to KS1 progression",
      "cypher": "MATCH path=(c1:Concept)-[:PREREQUISITE_OF]->(c2:Concept) WHERE c1.concept_id =~ '(CL|PSED|PD|LIT|EYMA|UW|EAD)-R-.*' RETURN path LIMIT 100",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "What does $concept lead to",
      "id": "tmpl:4",
      "createdAt": 1740268800000,
      "text": "What does $concept lead to",
      "cypher": "MATCH (c1:Concept) WHERE c1.concept_name CONTAINS $concept OR c1.concept_id = $concept MATCH path=(c1)-[:PREREQUISITE_OF*1..3]->(c2:Concept) RETURN path LIMIT 80",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Literacy EYFS to KS1",
      "id": "tmpl:5",
      "createdAt": 1740268800000,
      "text": "Literacy EYFS to KS1",
      "cypher": "MATCH path=(c1:Concept)-[:PREREQUISITE_OF]->(c2:Concept) WHERE c1.concept_id STARTS WITH 'LIT-R-' RETURN path LIMIT 80",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Maths EYFS to KS1",
      "id": "tmpl:6",
      "createdAt": 1740268800000,
      "text": "Maths EYFS to KS1",
      "cypher": "MATCH path=(c1:Concept)-[:PREREQUISITE_OF]->(c2:Concept) WHERE c1.concept_id STARTS WITH 'EYMA-R-' RETURN path LIMIT 80",
      "params": [],
