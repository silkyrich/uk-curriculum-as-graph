  "templates": [
    {
      "name": "Full profile for $year",
      "id": "tmpl:lp5",
      "createdAt": 1708387200000,
      "text": "Full profile for $year",
      "cypher": "MATCH (y:Year {year_id:$year})\nMATCH (y)-[:HAS_CONTENT_GUIDELINE]->(cg)\nMATCH (y)-[:HAS_PEDAGOGY_PROFILE]->(pp)\nMATCH (y)-[:HAS_FEEDBACK_PROFILE]->(fp)\nMATCH (y)-[:SUPPORTS_INTERACTION]->(it)\nOPTIONAL MATCH (pp)-[:USES_TECHNIQUE]->(pt)\nRETURN y, cg, pp, fp, it, pt",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Primary interactions for $year",
      "id": "tmpl:lp6",
      "createdAt": 1708387200000,
      "text": "Primary interactions for $year",
      "cypher": "MATCH (y:Year {year_id:$year})-[:SUPPORTS_INTERACTION {primary:true}]->(i:InteractionType)\nRETURN y, i",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "All maths tools",
      "id": "tmpl:lp7",
      "createdAt": 1708387200000,
      "text": "All maths tools",
      "cypher": "MATCH (y:Year)-[:SUPPORTS_INTERACTION]->(i:InteractionType {category:'maths_tool'})\nRETURN y, i",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Compare KS1 vs KS2 profiles",
      "id": "tmpl:lp8",
      "createdAt": 1708387200000,
      "text": "Compare KS1 vs KS2 profiles",
      "cypher": "MATCH (ks:KeyStage)-[:HAS_YEAR]->(y:Year)\nWHERE ks.key_stage IN ['KS1','KS2']\nMATCH (y)-[:HAS_CONTENT_GUIDELINE]->(cg)\nMATCH (y)-[:HAS_PEDAGOGY_PROFILE]->(pp)\nRETURN y, cg, pp",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Scaffolding by year",
      "id": "tmpl:lp9",
      "createdAt": 1708387200000,
      "text": "Scaffolding by year",
      "cypher": "MATCH (y:Year)-[:HAS_PEDAGOGY_PROFILE]->(pp:PedagogyProfile)\nRETURN y, pp ORDER BY y.year_number",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Pedagogy technique progression",
      "id": "tmpl:lp10",
      "createdAt": 1708387200000,
      "text": "Pedagogy technique progression",
      "cypher": "MATCH (pt:PedagogyTechnique)\nOPTIONAL MATCH (pt)-[:REQUIRES]->(req:PedagogyTechnique)\nRETURN pt, req",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "When is $technique introduced?",
      "id": "tmpl:lp11",
      "createdAt": 1708387200000,
      "text": "When is $technique introduced?",
      "cypher": "MATCH (pp:PedagogyProfile)-[:INTRODUCES_TECHNIQUE]->(pt:PedagogyTechnique {technique_id:$technique})\nMATCH (y:Year)-[:HAS_PEDAGOGY_PROFILE]->(pp)\nRETURN y, pp, pt",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Interactions for $subject",
      "id": "tmpl:lp12",
      "createdAt": 1708387200000,
      "text": "Interactions for $subject",
      "cypher": "MATCH (i:InteractionType)-[:SUPPORTS_LEARNING_OF]->(s:Subject {subject_id:$subject})\nRETURN i, s",
