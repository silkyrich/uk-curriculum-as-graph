  "templates": [
    {
      "name": "KS2 test coverage",
      "id": "tmpl:1",
      "createdAt": 1708387200000,
      "text": "KS2 test coverage",
      "cypher": "MATCH (tf:TestFramework)-[:HAS_PAPER]->(tp:TestPaper)-[:INCLUDES_CONTENT]->(cdc:ContentDomainCode) RETURN tf, tp, cdc LIMIT 200",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Concepts assessed in KS2",
      "id": "tmpl:2",
      "createdAt": 1708387200000,
      "text": "Concepts assessed in KS2",
      "cypher": "MATCH (cdc:ContentDomainCode)-[:ASSESSES_CONCEPT]->(c:Concept) RETURN cdc, c LIMIT 200",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Domains assessed in KS2",
      "id": "tmpl:3",
      "createdAt": 1708387200000,
      "text": "Domains assessed in KS2",
      "cypher": "MATCH (cdc:ContentDomainCode)-[:ASSESSES_DOMAIN]->(d:Domain) RETURN cdc, d LIMIT 200",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "$concept test questions",
      "id": "tmpl:4",
      "createdAt": 1708387200000,
      "text": "$concept test questions",
      "cypher": "MATCH (cdc:ContentDomainCode)-[:ASSESSES_CONCEPT]->(c:Concept) WHERE c.name CONTAINS $concept RETURN cdc, c LIMIT 100",
      "params": [],
      "hasCypherErrors": false
    },
    {
      "name": "Maths paper breakdown",
      "id": "tmpl:5",
      "createdAt": 1708387200000,
      "text": "Maths paper breakdown",
      "cypher": "MATCH (tf:TestFramework {subject:'Mathematics'})-[:HAS_PAPER]->(tp)-[:INCLUDES_CONTENT]->(cdc) OPTIONAL MATCH (cdc)-[:ASSESSES_CONCEPT]->(c:Concept) RETURN tf, tp, cdc, c LIMIT 200",
      "params": [],
      "hasCypherErrors": false
    }
  ],
